mixin calendar
  .calendar
    .day_names.cal_row
      div Mon
        span day
      div Tue
        span day
      div Wednes
        span day
      div Thur
        span day
      div Fri
        span day
      div Sat
        span day
      div Sun
        span day
    .cal_row(data-bind="foreach: Days")
      div(data-bind="css: MonthStatus")
        i(data-bind="text: Day")


