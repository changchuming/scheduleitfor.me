mixin calendar
  .calendar
    .day_names.cal_row
      div Sun
        span day
      div Mon
        span day
      div Tue
        span day
      div Wed
        span day
      div Thur
        span day
      div Fri
        span day
      div Sat
        span day
    .cal_row(data-bind="foreach: dates")
      div(data-bind="css: monthStatus")
        i(data-bind="text: day")


